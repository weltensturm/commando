

fibonacci = $a:
    a < 2 and a or (fibonacci a-1) + (fibonacci a-2)

check (fibonacci 10) == 55


fibonacciFast = $a:
    prev = 0
    cur = 0
    range a+1 | each $i v:
        if i == 0:
            cur = 0
        if i == 1:
            prev = 0
            cur = 1
        if i > 1:
            prev' = prev
            prev = cur
            cur = prev' + cur
    cur


fibonacciFast 1000 # doubles can't handle this though

